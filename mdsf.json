{
  "$schema": "https://raw.githubusercontent.com/hougesen/mdsf/main/schemas/v0.0.9/mdsf.schema.json",
  "format_finished_document": false,
  "javascript_runtime": "node",
  "languages": {
    "zig": "zigfmt",
    "shell": [["shfmt", "beautysh"]],
    "rust": "rustfmt",
    "swift": [["swift-format", "swiftformat"]],
    "c": "clang-format",
    "sql": [["sql-formatter", "sqlfluff"]],
    "css": [["prettier", "stylelint"]],
    "crystal": "crystal_format",
    "json": [["prettier", "biome", "deno_fmt", "clang-format"]],
    "lua": [["stylua", "luaformatter"]],
    "yaml": [["prettier", "yamlfmt", "yamlfix"]],
    "erlang": [["erlfmt", "efmt"]],
    "csharp": [["csharpier", "clang-format"]],
    "haskell": [["fourmolu", "ormolu", "hindent", "stylish-haskell"]],
    "ruby": [["rubocop", "rufo", "rubyfmt", "standardrb"]],
    "javascript": [
      ["prettier", "biome", "deno_fmt", "clang-format", "standardjs"]
    ],
    "typescript": [["prettier", "biome", "deno_fmt"]],
    "cpp": "clang-format",
    "ocaml": [["ocamlformat", "ocp-indent"]],
    "java": [["google-java-format", "clang-format"]],
    "elm": "elm-format",
    "d": "dfmt",
    "html": [["prettier", "djlint"]],
    "gleam": "gleam_format",
    "dart": "dart_format",
    "clojure": [["cljstyle", "joker"]],
    "python": [
      ["usort", "isort"],
      ["ruff", "blue", "black", "yapf", "autopep8", "pyink"]
    ],
    "elixir": "mix_format",
    "go": [
      ["gci", "goimports-reviser", "goimports"],
      ["gofumpt", "gofmt", "crlfmt"]
    ],
    "roc": "roc_format",
    "nim": "nimpretty",
    "toml": "taplo"
  }
}
