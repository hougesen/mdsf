{
  "$schema": "https://raw.githubusercontent.com/hougesen/mdsf/main/schemas/v0.0.3/mdsf.schema.json",
  "format_finished_document": false,
  "javascript_runtime": "node",
  "blade": {
    "enabled": true,
    "formatter": "blade-formatter"
  },
  "c": {
    "enabled": true,
    "formatter": "clang-format"
  },
  "clojure": {
    "enabled": true,
    "formatter": "cljstyle"
  },
  "cpp": {
    "enabled": true,
    "formatter": "clang-format"
  },
  "crystal": {
    "enabled": true,
    "formatter": "crystal_format"
  },
  "csharp": {
    "enabled": true,
    "formatter": [["csharpier", "clang-format"]]
  },
  "css": {
    "enabled": true,
    "formatter": "prettier"
  },
  "dart": {
    "enabled": true,
    "formatter": "dart_format"
  },
  "elixir": {
    "enabled": true,
    "formatter": "mix_format"
  },
  "elm": {
    "enabled": true,
    "formatter": "elm-format"
  },
  "erlang": {
    "enabled": true,
    "formatter": "erlfmt"
  },
  "fsharp": {
    "enabled": true,
    "formatter": "fantomas"
  },
  "gleam": {
    "enabled": true,
    "formatter": "gleam_format"
  },
  "go": {
    "enabled": true,
    "formatter": ["goimports", ["gofumpt", "gofmt"]]
  },
  "graphql": {
    "enabled": true,
    "formatter": "prettier"
  },
  "groovy": {
    "enabled": true,
    "formatter": "npm-groovy-lint"
  },
  "haskell": {
    "enabled": true,
    "formatter": [["fourmolu", "ormolu", "hindent"]]
  },
  "html": {
    "enabled": true,
    "formatter": "prettier"
  },
  "java": {
    "enabled": true,
    "formatter": [["google-java-format", "clang-format"]]
  },
  "javascript": {
    "enabled": true,
    "formatter": [["prettier", "biome", "deno_fmt", "clang-format"]]
  },
  "json": {
    "enabled": true,
    "formatter": [["prettier", "biome", "deno_fmt", "clang-format"]]
  },
  "julia": {
    "enabled": true,
    "formatter": "juliaformatter.jl"
  },
  "just": {
    "enabled": true,
    "formatter": "just_fmt"
  },
  "kotlin": {
    "enabled": true,
    "formatter": [["ktlint", "ktfmt"]]
  },
  "lua": {
    "enabled": true,
    "formatter": [["stylua", "luaformatter"]]
  },
  "markdown": {
    "enabled": true,
    "formatter": "prettier"
  },
  "nim": {
    "enabled": true,
    "formatter": "nimpretty"
  },
  "objective_c": {
    "enabled": true,
    "formatter": "clang-format"
  },
  "ocaml": {
    "enabled": true,
    "formatter": "ocamlformat"
  },
  "protobuf": {
    "enabled": true,
    "formatter": [["buf", "clang-format"]]
  },
  "perl": {
    "enabled": true,
    "formatter": "perltidy"
  },
  "purescript": {
    "enabled": true,
    "formatter": "purs-tidy"
  },
  "python": {
    "enabled": true,
    "formatter": [
      ["usort", "isort"],
      ["ruff", "blue", "black", "yapf", "autopep8"]
    ]
  },
  "rescript": {
    "enabled": true,
    "formatter": "rescript_format"
  },
  "roc": {
    "enabled": true,
    "formatter": "roc_format"
  },
  "ruby": {
    "enabled": true,
    "formatter": [["rubocop", "rufo", "rubyfmt", "standardrb"]]
  },
  "rust": {
    "enabled": true,
    "formatter": "rustfmt"
  },
  "scala": {
    "enabled": true,
    "formatter": "scalafmt"
  },
  "shell": {
    "enabled": true,
    "formatter": [["shfmt", "beautysh"]]
  },
  "swift": {
    "enabled": true,
    "formatter": "swiftformat"
  },
  "sql": {
    "enabled": true,
    "formatter": [["sql-formatter", "sqlfluff"]]
  },
  "toml": {
    "enabled": true,
    "formatter": "taplo"
  },
  "typescript": {
    "enabled": true,
    "formatter": [["prettier", "biome", "deno_fmt"]]
  },
  "vue": {
    "enabled": true,
    "formatter": "prettier"
  },
  "xml": {
    "enabled": true,
    "formatter": [["xmllint", "xmlformat"]]
  },
  "yaml": {
    "enabled": true,
    "formatter": [["prettier", "yamlfmt", "yamlfix"]]
  },
  "zig": {
    "enabled": true,
    "formatter": "zigfmt"
  }
}
