{
  "$schema": "../tool.schema.json",
  "binary": "phpinsights",
  "categories": ["linter"],
  "commands": {
    "fix": {
      "arguments": ["fix", "$PATH", "--no-interaction", "--quiet"]
    }
  },
  "description": "Instant PHP quality checks from your console",
  "homepage": "https://github.com/nunomaduro/phpinsights",
  "languages": ["php"],
  "packages": {
    "composer": {
      "binary": "phpinsights",
      "package": "nunomaduro/phpinsights"
    }
  }
}
