{
  "$schema": "../tool.schema.json",
  "binary": "vacuum",
  "categories": ["linter"],
  "commands": {
    "lint": {
      "arguments": ["lint", "$PATH"],
      "tests": []
    }
  },
  "description": "vacuum is the worlds fastest OpenAPI 3, OpenAPI 2 / Swagger linter and quality analysis tool",
  "homepage": "https://github.com/daveshanley/vacuum",
  "languages": ["json", "openapi", "yaml"],
  "packages": {
    "brew": {
      "package": "daveshanley/vacuum/vacuum"
    },
    "mise": {
      "package": "vacuum"
    },
    "npm": {
      "package": "@quobix/vacuum"
    }
  }
}
