{
  "$schema": "../tool.schema.json",
  "binary": "gdformat",
  "categories": ["formatter"],
  "commands": {
    "": {
      "arguments": ["$PATH"]
    }
  },
  "description": "GDScript formatter",
  "homepage": "https://github.com/scony/godot-gdscript-toolkit",
  "languages": ["gdscript"],
  "packages": {
    "brew": {
      "package": "gdtoolkit"
    },
    "pip": {
      "executable": "gdformat",
      "package": "gdtoolkit"
    }
  }
}
