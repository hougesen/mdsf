{
  "binary": "prettier",
  "categories": ["formatter"],
  "commands": {
    "": [
      "format",
      "--embedded-language-formatting",
      "off",
      "--log-level",
      "error",
      "--write",
      "$PATH"
    ]
  },
  "description": "Prettier is an opinionated code formatter.",
  "languages": ["javascript", "typescript", "json", "html", "css"],
  "npm": "prettier",
  "website": "https://github.com/prettier/prettier"
}
