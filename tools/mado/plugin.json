{
  "$schema": "../tool.schema.json",
  "binary": "mado",
  "categories": ["linter"],
  "commands": {
    "check": {
      "arguments": ["check", "$PATH"],
      "ignore_output": true,
      "tests": [
        {
          "language": "markdown",
          "test_input": "# Hello world\n\n- Hello\n- world\n\n",
          "test_output": null
        }
      ]
    }
  },
  "description": "A fast Markdown linter written in Rust",
  "homepage": "https://github.com/akiomik/mado",
  "languages": ["markdown"]
}
