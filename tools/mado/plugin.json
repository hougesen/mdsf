{
  "$schema": "../tool.schema.json",
  "binary": "mado",
  "categories": ["linter"],
  "commands": {
    "check": {
      "arguments": ["check", "$PATH"],
      "tests": [
        {
          "language": "markdown",
          "test_input": "# Hello world\n\n- Hello\n- world\n\n",
          "test_output": "# Hello world\n\n- Hello\n- world\n\n"
        }
      ]
    }
  },
  "description": "A fast Markdown linter written in Rust",
  "homepage": "https://github.com/akiomik/mado",
  "languages": ["markdown"],
  "packages": {
    "brew": {
      "name": "mado",
      "tap": "akiomik/mado https://github.com/akiomik/mado.git"
    }
  }
}
