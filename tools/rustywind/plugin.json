{
  "$schema": "../tool.schema.json",
  "binary": "rustywind",
  "categories": ["formatter"],
  "commands": {
    "": {
      "arguments": ["--write", "$PATH"],
      "tests": [
        {
          "language": "html",
          "test_input": "<div class=\"flex-col flex\"></div>\n",
          "test_output": "<div class=\"flex flex-col\"></div>\n"
        }
      ]
    }
  },
  "description": "CLI for organizing Tailwind CSS classes",
  "homepage": "https://github.com/avencera/rustywind",
  "languages": ["html"],
  "packages": {
    "cargo": {
      "package": "rustywind"
    },
    "npm": {
      "package": "rustywind"
    }
  }
}
